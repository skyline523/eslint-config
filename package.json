{
  "name": "@leet11/eslint-config",
  "type": "module",
  "version": "0.0.4",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.6.2",
  "description": "ESlint config for Leet",
  "author": "Leet <1414395519@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/skyline523/eslint-config/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skyline523/eslint-config.git"
  },
  "bugs": "https://github.com/skyline523/eslint-config/issues",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --clean --dts",
    "dev": "npx @eslint/config-inspector",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "tag": "bumpp",
    "release": "pnpm tag && pnpm publish",
    "start": "tsx src/index.ts",
    "gen": "tsx scripts/typegen.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "^9.10.0",
    "eslint-plugin-tailwindcss": "^3.18.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-tailwindcss": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-typegen": "^2.1.0"
  },
  "devDependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@antfu/ni": "^24.1.0",
    "@antfu/utils": "^9.1.0",
    "@clack/prompts": "^0.10.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^22.13.10",
    "bumpp": "^10.1.0",
    "eslint": "^9.22.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "lint-staged": "^15.5.0",
    "local-pkg": "^1.1.1",
    "simple-git-hooks": "^2.11.1",
    "tsdown": "^0.11.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "vitest-package-exports": "^0.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
